/* pages/group/groups.wxss */
.container {
    min-height: 100%;
}

.cell {
    padding: 0 30rpx;
}

.button--btn {
    margin: 0 auto;
    margin-top: 117rpx;
}

.member {
    width: 20%;
    margin-bottom: 20rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.context {
    display: flex;
    flex-wrap: wrap;
    background: #f4f4f4 !important;
    box-shadow: inset 0 0 4rpx rgba(0, 0, 0, 0.25);
}

.member .avatar {
    width: 72rpx;
    height: 72rpx;
}